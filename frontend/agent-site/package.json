{
  "name": "my-razzle-app",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all -p watch-css start-razzle",
    "start-razzle": "razzle start",
    "build": "npm run build-css && razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src\\App.scss -o src --watch --recursive"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "razzle": "^3.0.0",
    "react": "^16.12.0",
    "react-document-meta": "^3.0.0-beta.2",
    "react-dom": "^16.12.0",
    "react-inlinesvg": "^1.2.0",
    "react-paginate": "^6.3.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-form": "^8.3.0"
  }
}
